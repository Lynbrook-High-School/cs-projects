{

    "build": {
        "context": ".",
        "dockerfile": "Dockerfile"
    },

    "customizations": {
        "vscode": {
            "extensions": [
                "/opt/codespace/extensions/lynbrook-cs-0.1.0.vsix",
                "vscjava.vscode-java-pack",
                "oliver-ni.web-cat-vscode"
            ],
            "settings": {
                "files.autoSave": "off",
                "files.exclude": {
                    "Dockerfile": true,
                    ".gitignore": true,
                    ".devcontainer.json": true,
